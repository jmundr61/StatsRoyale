        $('#missing_selected').slideDown();

        if (missing.length == 0) {
            $('#missing_selected').slideUp();
            $('#missing_nothing').fadeIn();
        }

        $('#deck_h1').addClass('text-white').removeClass('text-muted');
        $('#deck_info').addClass('lead text-white').removeClass('text-muted');
        $('#deck_selected').slideDown();
        $('#deck_picker').slideDown();
        $('#deck_stats').fadeIn();

        $('#btn_edit_arena').fadeIn();
        $('#btn_edit_missing').fadeIn();

        $('#btn_restart').fadeIn();
    }

    updateLinks();


    if (deck) {
        // startup
        $('#cards_count').text(cards.length);

        for (var i = 0, len = cards.length; i < len; i++) {
            $('#deck_selected .card_link[data-hash=' + cards[i] + ']').show();
            $('img#deck_card_' + cards[i]).addClass('active');
        }

        for (var i = 0, len = missing.length; i < len; i++) {
            $('#missing_selected .card_link[data-hash=' + missing[i] + ']').show();
            $('img#missing_card_' + missing[i]).addClass('active');
        }

        if (cards.length == 8) {
            $('#button_analyze').removeClass('disabled');
        }

        $('#arena_h1').addClass('text-muted').removeClass('text-white');
        $('#arena_info').hide();
//        $('#arena_selected').html($('#arena_picker a[data-arena='+arena+']').html()).css('top', '-15px').fadeIn();
        $('#arena_selected').text( "Arena" + " " + arena).fadeIn();
        $('#arena_picker').hide();

        $('#card_picker_deck .card_link').each(function () {
            if (($(this).data('arena') > arena && $(this).data('rarity') < 4) || ($(this).data('rarity') == 4 && arena < 4) || missing.indexOf($(this).data('hash')) >= 0) {
                $(this).hide();
            }
        });

        $('#missing_info').hide();
        $('#missing_selected').slideDown();

        if (missing.length == 0) {
            $('#missing_selected').slideUp();
            $('#missing_nothing').fadeIn();
        }

        $('#deck_h1').addClass('text-white').removeClass('text-muted');
        $('#deck_info').addClass('lead text-white').removeClass('text-muted');
        $('#deck_selected').slideDown();
        $('#deck_picker').slideDown();
        $('#deck_stats').fadeIn();

        $('#btn_edit_arena').fadeIn();
        $('#btn_edit_missing').fadeIn();

        $('#btn_restart').fadeIn();

        if (hilite) {
            for (var i = 0, len = hilite.length; i < len; i++) {
                $('img#deck_card_' + hilite[i]).parent().addClass('hilited');
            }
        }

        updateLinks();
        calculateAec();
    }

    $('#arena_picker a[data-arena]').click(function (event) {
        event.preventDefault();

        arena = $(this).data('arena');

        $('#arena_h1').addClass('text-muted').removeClass('text-white');
        $('#arena_selected').text( "Arena" + " " + arena).addClass('text-white').removeClass('text-muted').fadeIn();
        $('#arena_info').slideUp();
        $('#arena_picker').slideUp();

        $('#card_picker_missing .card_link').each(function () {
            $(this).show();
        });
        $('#card_picker_missing .card_link').each(function () {
            if (($(this).data('arena') > arena && $(this).data('rarity') < 4) || ($(this).data('rarity') == 4 && arena < 4)) {
                $(this).hide();
            }
        });

        if (missing.length > 0 || cards.length > 0) {

            $('#deck_h1').addClass('text-white').removeClass('text-muted');
            $('#deck_info').addClass('lead text-white').removeClass('text-muted').fadeIn();
            $('#deck_selected').slideDown();
            $('#deck_picker').slideDown();
            $('#deck_stats').fadeIn();

            $('#card_picker_deck .card_link').each(function () {
                $(this).show();
            });
            $('#card_picker_deck .card_link').each(function () {
                if (($(this).data('arena') > arena && $(this).data('rarity') < 4) || ($(this).data('rarity') == 4 && arena < 4) || missing.indexOf($(this).data('hash')) >= 0) {
                    $(this).hide();
                }
            });

            $('#btn_edit_missing').fadeIn();
        } else {
            $('#missing_picker').slideDown();
            $('#missing_h1').addClass('text-white').removeClass('text-muted');
            $('#missing_info').addClass('lead text-white').removeClass('text-muted').slideDown();
            $('#missing_selected').slideDown();
        }

        $('#btn_restart').fadeIn();
        $('#btn_edit_arena').fadeIn();

        Cookies.set("arena", $(this).data('arena'));

        updateLinks();
    });

    $('#card_picker_missing .card_link').click(function (event) {
        event.preventDefault();

        var hash = $(this).data('hash');
        var child = $(this).children('img');
        var pos = missing.indexOf(hash);

        if (pos < 0) {
            if (missing.length < 53) {
                missing.push(hash);
                child.addClass('active');
                $('#missing_selected .card_link[data-hash=' + hash + ']').show();
            }
        } else if (pos >= 0) {
            missing.splice(pos, 1);
            child.removeClass('active');
            $('#missing_selected .card_link[data-hash=' + hash + ']').hide();
        }
        updateLinks();
    });

    $('#button_2_continue').click(function (event) {
        event.preventDefault();

        $('#missing_h1').addClass('text-muted').removeClass('text-white');
        $('#missing_info').slideUp();
        $('#missing_picker').slideUp();

        $('#card_picker_deck .card_link').each(function () {
            $(this).show();
        });
        $('#card_picker_deck .card_link').each(function () {
            if (($(this).data('arena') > arena && $(this).data('rarity') < 4) || ($(this).data('rarity') == 4 && arena < 4) || missing.indexOf($(this).data('hash')) >= 0) {
                $(this).hide();
            }
        });

        if (missing.length == 0) {
            $('#missing_selected').slideUp();
            $('#missing_nothing').fadeIn();
        }

        $('#deck_h1').addClass('text-white').removeClass('text-muted');
        $('#deck_info').addClass('lead text-white').removeClass('text-muted').fadeIn();
        $('#deck_selected').slideDown();
        $('#deck_picker').slideDown();
        $('#deck_stats').fadeIn();

        Cookies.set("missing", missing);

        $('#btn_edit_arena').fadeIn();
        $('#btn_edit_missing').fadeIn();

        updateLinks();
    });

    $('#btn_edit_arena').click(function (event) {
        event.preventDefault();

        $('#btn_edit_arena').fadeOut();
        $('#btn_edit_missing').fadeOut();

        $('#deck_h1').addClass('text-muted').removeClass('text-white');
        $('#deck_info').fadeOut();
        $('#deck_picker').slideUp();
        $('#deck_stats').fadeOut();

        $('#missing_h1').addClass('text-muted').removeClass('text-white');
        $('#missing_info').slideUp();
        $('#missing_picker').slideUp();

        $('#arena_h1').addClass('text-white').removeClass('text-muted');
        $('#arena_selected').addClass('text-muted').removeClass('text-white');
        $('#arena_info').slideDown();
        $('#arena_picker').slideDown();

        updateLinks();
    });

    $('#btn_edit_missing').click(function (event) {
        event.preventDefault();

        $('#btn_edit_arena').fadeOut();
        $('#btn_edit_missing').fadeOut();

        for (var i = 0, len = missing.length; i < len; i++) {
            $('#missing_selected .card_link[data-hash=' + missing[i] + ']').show();
            $('img#missing_card_' + missing[i]).addClass('active');
        }

        $('#deck_h1').addClass('text-muted').removeClass('text-white');
        $('#deck_info').fadeOut();
        $('#deck_picker').slideUp();
        $('#deck_stats').fadeOut();

        $('#missing_picker').slideDown();
        $('#missing_h1').addClass('text-white').removeClass('text-muted');
        $('#missing_info').addClass('lead text-white').removeClass('text-muted').slideDown();
        $('#missing_selected').slideDown();
        $('#missing_nothing').fadeOut();

        updateLinks();
    });

    $('#card_picker_deck .card_link').click(function (event) {
        event.preventDefault();

        var hash = $(this).data('hash');
        var child = $(this).children('img');
        var pos = cards.indexOf(hash);

        if (pos < 0) {
            if (cards.length < 8) {
                cards.push(hash);
                child.addClass('active');
                $('#deck_selected .card_link[data-hash=' + hash + ']').show();
            }
        } else if (pos >= 0) {
            cards.splice(pos, 1);
            child.removeClass('active');
            $('#deck_selected .card_link[data-hash=' + hash + ']').hide();
        }

        $('#cards_count').text(cards.length);

        if (cards.length == 8) {
            $('#button_analyze').removeClass('disabled');
        } else {
            $('#button_analyze').addClass('disabled');
        }

        updateLinks();
        calculateAec();
    });

    $('#btn_restart_a').click(function (event) {
        event.preventDefault();

        Cookies.remove("arena");
        Cookies.remove("missing");

        window.location.href = $(this).attr("href");
    });


    function updateLinks() {
        var get_deck = "?deck=" + cards.join('-');
        var get_missing = (missing.length > 0) ? "&missing=" + missing.join('-') : "";
        var get_arena = (arena != 11) ? "&arena=" + arena : "";
        var get_hilite = (hilite.length > 0) ? "&hilite=" + hilite.join('-') : "";


        $("#permalink").attr("href", link_perma + get_deck + get_missing + get_arena + get_hilite);
        $("#button_analyze").attr("href", link_analyze + get_deck + get_missing + get_arena);

        window.history.replaceState( { } , null, link_perma + get_deck + get_missing + get_arena + get_hilite);
    }

    function calculateAec() {
        if (cards.length < 8) {
            $("#aec").text("…");
        } else {
            var result = 0;
            var sum = 0;
            for (var i = 0, len = cards.length; i < len; i++) {
                sum += ec[cards[i]];
            }

            if (cards.includes('Mirror')) {
                result = Math.floor(((sum + 1.99) / cards.length) * 10) / 10;
            }
            else {
                result = sum / cards.length;
            }

            $("#aec").text(result.toFixed(1));
        }
    }

        });
    </script>
</body>
</html>
